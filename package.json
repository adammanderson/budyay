{
  "name": "budyay",
  "version": "0.0.1",
  "description": "Connecting buddies",
  "main": "index.js",
  "engines": {
    "node": "10"
  },
  "scripts": {
    "start": "next start",
    "dev": "next src/app",
    "build:app": "next build src/app",
    "build:server": "babel src/functions --out-dir dist/functions",
    "build": "yarn run build:app && yarn run build:server",
    "copy-public": "cpx \"src/app/public/**/*.*\" dist/public -C",
    "copy-deps": "cpx \"*{package.json,package-lock.json,yarn.lock}\" dist/functions -C",
    "predeploy": "rimraf dist/ && yarn run build && yarn run copy-public && yarn run copy-deps",
    "deploy": "cross-env NODE_ENV=production firebase deploy --only functions,hosting --debug"
  },
  "author": "Adam Anderson",
  "license": "ISC",
  "dependencies": {
    "firebase-admin": "8.10.0",
    "firebase-functions": "3.6.0",
    "next": "9.3.4",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.0",
    "@babel/preset-env": "7.9.5",
    "@types/enzyme-adapter-react-16": "1.0.6",
    "@types/jest": "25.2.1",
    "@types/node": "13.11.1",
    "@types/react": "16.9.34",
    "@types/react-dom": "16.9.6",
    "@typescript-eslint/eslint-plugin": "2.27.0",
    "@typescript-eslint/parser": "2.27.0",
    "babel-loader": "8.1.0",
    "cpx": "1.5.0",
    "cross-env": "7.0.2",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "enzyme-to-json": "3.4.4",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.1.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-react-hooks": "3.0.0",
    "jest": "25.3.0",
    "rimraf": "3.0.2",
    "ts-jest": "25.3.1",
    "typescript": "3.8.3"
  }
}
